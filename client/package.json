    "localforage": "^1.7.1",
    "normalize.css": "^7.0.0",
    "oauth-open": "^1.0.3",
    "shortid": "^2.2.8",
    "vue": "^2.5.16",
    "vue-color": "^2.4.6",
    "vue-directive-tooltip": "^1.4.5",
    "vue-mdc-adapter": "^0.15.3",
    "vue-router": "^2.8.1",
    "vuex": "^2.5.0"
  },
